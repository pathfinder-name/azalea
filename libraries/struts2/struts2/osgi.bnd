Embed-Dependency: \
 xwork-core;groupId=org.apache.struts.xwork;inline=true,\
 struts2-core;groupId=org.apache.struts;inline=true
 
Export-Package: \
 org.apache.commons.lang.xwork*;version="${bundle.version}"\
